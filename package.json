{
  "name": "kitty-mock",
  "version": "0.0.1",
  "description": "API Mocker",
  "main": "server.ts",
  "scripts": {
    "start": "node main.js",
    "test:mocha": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"CommonJS\"}' mocha $npm_package_config_mocha_reqs",
    "test:unit": "npm -s run test:mocha -- --reporter progress tests/unit/*.test.ts",
    "test:unit:watch": "npm -s run test:mocha -- --watch --reporter progress tests/unit/*.test.ts",
    "test:functional": "npm -s run test:mocha -- --reporter progress tests/functional/*.test.ts",
    "test:functional:watch": "npm -s run test:mocha -- --watch --reporter progress tests/functional/*.test.ts",
    "test": "npm -s run test:functional && npm -s run test:unit"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/nTopus/kitty-mock.git"
  },
  "keywords": [
    "api",
    "mock",
    "server"
  ],
  "config": {
    "mocha_reqs": "-r ts-node/register -r chai/register-assert -r chai/register-should -r chai/register-expect --extension js,ts"
  },
  "author": "nTopus Possibilidades",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/nTopus/kitty-mock/issues"
  },
  "homepage": "https://gitlab.com/nTopus/kitty-mock#readme",
  "dependencies": {
    "@types/node": "^12.12.7",
    "chalk": "^3.0.0",
    "dotenv": "^8.2.0",
    "flatted": "^2.0.1",
    "lodash": "^4.17.15",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.5",
    "@types/mocha": "^5.2.7",
    "axios": "^0.19.0",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "ts-node": "^8.5.0"
  }
}
